{
  "name": "@fure/examples-demo",
  "version": "0.1.1",
  "description": "Functional Reactive Game Framework (examples/demo)",
  "private": true,
  "homepage": "https://zhengxiaoyao0716.github.io/furegame",
  "license": "MIT",
  "dependencies": {
    "@fure/core": "^0.1.1",
    "@fure/main": "^0.1.1",
    "carlo": "^0.9.45",
    "react-router-dom": "5.1",
    "rxjs": "^6.5.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "postbuild": "babel src/main -d build/main --extensions \".js\" --config-file ../../.babelrc --copy-files",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "prepack": "npm run build",
    "pack": "pkg . --output dist/demo.exe --targets x86",
    "postpack": "fure-main build:launch dist/demo.exe --icon public/favicon.ico"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "bin": "build/main/index.js",
  "pkg": {
    "scripts": "build/**/*.js",
    "assets": [
      "build/**/*",
      "!build/main/**/*.js"
    ]
  },
  "devDependencies": {
    "@babel/node": "^7.4.5",
    "@fure/view": "^0.1.1",
    "@types/carlo": "^0.9.0",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "gh-pages": "^2.0.1",
    "pkg": "^4.4.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.1.0",
    "react-scripts": "3.1.1"
  }
}
